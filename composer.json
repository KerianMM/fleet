{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "behat/behat": "*",
        "phpunit/phpunit": "^10",
        "infection/infection": "^0.26"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "infection/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Kerianmm\\Fleet\\": "src/",
            "Kerianmm\\Fleet\\Tests\\": "tests/"
        }
    }
}
